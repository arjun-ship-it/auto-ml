# Gemini API
GOOGLE_API_KEY=your-google-api-key-here
GEMINI_MODEL=gemini-2.0-flash
MAX_TOKENS=4096

# App Settings
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Database
DATABASE_URL=sqlite+aiosqlite:///./automl.db

# File Storage
UPLOAD_DIR=./uploads
MODELS_DIR=./trained_models

# Docker Execution
DOCKER_ENABLED=true
DOCKER_IMAGE=automl-sandbox:latest
EXECUTION_TIMEOUT=300
